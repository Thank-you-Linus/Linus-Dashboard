  },
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/Thank-you-Linus/Linus-Dashboard"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.17",
    "@types/swiper": "^6.0.0",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-private-methods": "^7.24.7",
    "@babel/plugin-transform-runtime": "7.28.3",
    "@babel/preset-env": "7.28.3",
    "@babel/runtime": "7.28.4",
    "@rspack/cli": "^0.7.5",
    "@rspack/core": "1.5.5",
    "@rspack/dev-server": "1.1.4",
    "@types/lodash.merge": "^4.6.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-loader": "10.0.0",
    "concurrently": "^8.2.2",
    "core-js": "3.45.1",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-lit": "2.1.1",
    "eslint-plugin-unused-imports": "4.2.0",
    "eslint-plugin-wc": "3.0.1",
    "fancy-log": "^2.0.0",
    "gulp": "5.0.1",
    "home-assistant-js-websocket": "9.5.0",
    "ignore-loader": "^0.1.2",
    "rimraf": "^5.0.5",
    "superstruct": "2.0.2",
    "ts-loader": "^9",
    "ts-node": "^10",
    "typescript": "5.9.2",
    "version-bump-prompt": "^6"
  },
  "scripts": {
    "build-dev": "rspack build --config rspack.dev.config.cjs --watch",
    "build": "rspack build --config rspack.config.cjs",
    "build:prod": "rspack build --config rspack.config.cjs --mode production",
    "build:analyze": "STATS=1 npm run build",
    "build:test": "rspack build --config rspack.config.cjs --dry-run",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "bump": "bump --preid alpha package.json package-lock.json README.md ./custom_components/linus_dashboard/manifest.json ./custom_components/linus_dashboard/const.py ./src/linus-strategy.ts"
  }
}
